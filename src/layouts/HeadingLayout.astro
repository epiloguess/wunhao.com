---
interface Props {
  title: string;
}
const { title } = Astro.props;

import Layout from "./BaseLayout.astro";
---

<Layout title={title}>
  <section class="flex flex-col gap-10">
    <header>
      <h2 class="text-4xl font-bold">{title}</h2>
    </header>
    <slot />
  </section>
</Layout>
<style></style>
